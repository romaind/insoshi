<div class="tab_title"><h4>Project content</h4></div>
<div class="content">
	by <%=link_to @project.person.first_name + " " + @project.person.name, person_path(@project.person) %>
	<table>
    	<%- i=0 %>
    	<%- unless @project.creations.nil? %>
    		<%- @project.creations.each_index do |i| %>
    		  <%- crea = @project.creations[i]  -%>
       			<%- if i%3==0 %>
       				<tr>
        		<%- end %>
          				<td>
              				<%= link_to(image_tag(crea.asset.url(:thumb)), person_project_creation_path(current_person, @project, crea), :id => i, :class => dom_class(crea, :thumb)) %>
          				</td>
          		<%- i+=1 %>
          		<%- if i%3==0 %>
            		</tr>
          		<%- end %>
        	<%- end %>
      	<%- end %>
      	<%- while i<12 %>
        	<%- if i%6==0 %>
          		<tr>
        	<%- end %>
        			<td>
          				<%=image_tag "defaults/minipreview.gif", :class => "img_preview", :width => "57px" %>
        			</td>
        	<%- i+=1 %>
        	<%- if i%3==0 %>
          		</tr>
        	<%- end %>
      	<%- end %>
	</table>
	<%= cc_icons(@project.common) %>
</div>