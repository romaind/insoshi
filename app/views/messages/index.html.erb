<%- column_div :type => :primary do -%>
<div id="inbox">
  <h4>MESSAGES</h4>
  <div id="messages">

  <%= render :partial => 'message_nav' %>
  
  <%- if @messages.empty? -%>
    <h3 class="blankslate">No messages (yet!)</h3>
  <%- else -%>
    <%= will_paginate %>
    <%= render :partial => "message", :collection => @messages %>
    <%= will_paginate %>
  <%- end -%>
  </div>
</div>

<%- end -%>