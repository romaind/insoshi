<h1>New project</h1>

<%= error_messages_for :project %>

<% form_for(@project, :url => person_projects_path) do |f| %>
  <p>
    <b>Title</b><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <b>Views</b><br />
    <%= f.text_field :views %>
  </p>

  <p>
    <b>Licence</b><br />
    <%= f.text_area :licence %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', person_projects_path %>
